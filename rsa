#!/usr/bin/python3
import sys

def is_prime(n):
    if n <= 1:
        return False
    if n <= 3:
        return True
    if n % 2 == 0 or n % 3 == 0:
        return False
    i = 5
    while i * i <= n:
        if n % i == 0 or n % (i + 2) == 0:
            return False
        i += 6
    return True

def factorize_number(n):
    for i in range(2, n + 1):
        if n % i == 0 and is_prime(i) and is_prime(n // i):
            return (i, n // i)

def main(input_file):
    with open(input_file, 'r') as file:
        for line in file:
            try:
                n = int(line)
                if n > 1:
                    factors = factorize_number(n)
                    print(f"{n}={factors[0]}*{factors[1]}")
            except ValueError:
                continue

if __name__ == "__main__":
    if len(sys.argv) != 2:
        print("Usage: factors <file>")
        sys.exit(1)

    input_file = sys.argv[1]
    main(input_file)
